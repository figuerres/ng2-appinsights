{
  "name": "ng2-appinsights",
  "version": "0.0.1-alpha.2",
  "description": "App Insights module for Angular 2.0.x",
  "main": "bundles/ng2-appinsights.min.js",
  "scripts": {
    "build": "node_modules/.bin/webpack -p",
    "start": "node_modules/.bin/webpack --progress --colors --watch",
    "dev": "node_modules/.bin/webpack",
    "test:e2e": "karma start karma.config.js --browsers PhantomJS",
    "test:e2e:chrome": "karma start karma.config.js --browsers Chrome",
    "prepublish": "npm run build && npm run dev",
    "test:stop": "karma stop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harry671003/ng2-appinsights.git"
  },
  "keywords": [
    "angular2",
    "appinsights",
    "logging",
    "ng2",
    "ng2-appinsights",
    "appinsights-ng2",
    "appinsights-angular2"
  ],
  "files": [
    "bundles"
  ],
  "author": "Harry John",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/harry671003/ng2-appinsights/issues"
  },
  "homepage": "https://github.com/harry671003/ng2-appinsights#readme",
  "devDependencies": {
    "@types/core-js": "^0.9.35",
    "@types/jasmine": "^2.5.38",
    "@types/node": "0.0.2",
    "jasmine": "2.5.2",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "1.0.1",
    "karma-htmlfile-reporter": "0.3.4",
    "karma-jasmine": "1.0.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-typescript": "2.1.4",
    "karma-webpack": "1.8.0",
    "ts-loader": "^1.2.2",
    "tslint": "^4.0.2",
    "tslint-loader": "^3.2.1",
    "typescript": "^2.0.10",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "@angular/core": "2.2.4",
    "@angular/common": "2.2.4",
    "@angular/compiler": "2.2.4",
    "@angular/platform-browser": "2.2.4",
    "@angular/platform-browser-dynamic": "2.2.4",
    "applicationinsights-js": "^1.0.4",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.21",
    "es6-shim": "0.35.2"
  }
}
